/**index.wxss**/

.container{
  height: 1500rpx;
}
.container .search{
  width: 735rpx;
  height: 65rpx;
  padding: 12.5rpx 0 12.5rpx 15rpx;
  background: #55ab9b;
}
.container .search-left{
  background: rgb(138, 215, 201);
  text-align: left;
}
.container .search-left input{
  display: inline-block;
  font-size: 26rpx;
}
.search-placeholder{
  color: #54a394;
  line-height: 20rpx;
}
.container .search .search-left image{
  display: inline-block;
  width: 35rpx;
  height: 35rpx;
  padding: 15rpx 15rpx 15rpx 20rpx;
}

.container .search .search-right image{
  width: 45rpx;
  height: 45rpx;
  padding: 10rpx;
}

.container{
  padding: 0;
  font-size: 14rpx;
  background: #90d5c8;
  color: #000;
}

.item-container
{
  display:inline-block;
  width:100%
}
.item_value{
  font-size:33rpx;
  display:inline-block;
  vertical-align:middle;
}



.time_value{
  display:inline-block;
  float:right;
  width:50%;
  font-size:40rpx;
  text-align: right;
  padding-top:50rpx;

}
/*feed-item part is in app.wxss for multiplexing*/